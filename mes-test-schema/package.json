{
  "name": "@dvsa/mes-test-schema",
  "version": "3.2.0",
  "description": "Domain model for data associated with tests administered by the Mobile Examiners Service",
  "scripts": {
    "generate": "ts-node generateSchemas.ts generate",
    "generate-common": "json2ts -i categories/common/index.json -o categories/common/index.d.ts --unreachableDefinitions",
    "generate-cat-be": "json2ts -i categories/BE/partial.json -o categories/BE/partial.d.ts --unreachableDefinitions",
    "generate-cat-b": "json2ts -i categories/B/partial.json -o categories/B/partial.d.ts --unreachableDefinitions",
    "clean": "ts-node generateSchemas.ts clean",
    "test": "json2ts -i categories/BE/partial.json -o categories/BE/partial.d.ts --unreachableDefinitions"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dvsa/mes-api-definitions.git"
  },
  "keywords": [
    "dvsa"
  ],
  "author": "DVSA",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dvsa/mes-api-definitions/issues"
  },
  "homepage": "https://github.com/dvsa/mes-api-definitions#readme",
  "devDependencies": {
    "deepmerge": "^4.1.1",
    "json-schema-to-typescript": "^6.1.3",
    "pretty-js": "^0.2.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.4"
  }
}
